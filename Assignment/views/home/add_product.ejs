<html>

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="Start your development with a Dashboard for Bootstrap 4.">
  <meta name="author" content="Creative Tim">
  <title>Argon Dashboard - Free Dashboard for Bootstrap 4</title>
  <!-- Favicon -->
  <link rel="icon" href="/abc/img/brand/favicon.png" type="image/png">
  <!-- Fonts -->
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700">
  <!-- Icons -->
  <link rel="stylesheet" href="/abc/vendor/nucleo/css/nucleo.css" type="text/css">
  <link rel="stylesheet" href="/abc/vendor/@fortawesome/fontawesome-free/css/all.min.css" type="text/css">
  <!-- Argon CSS -->
  <link rel="stylesheet" href="/abc/css/argon.css?v=1.2.0" type="text/css">
</head>

<body>
  <!-- Sidenav -->
  <nav class="sidenav navbar navbar-vertical  fixed-left  navbar-expand-xs navbar-light bg-white" id="sidenav-main">
    <div class="scrollbar-inner">
      <!-- Brand -->
      <div class="sidenav-header  align-items-center">
        <a class="navbar-brand" href="javascript:void(0)">
          <img src="/abc/img/brand/blue.png" class="navbar-brand-img" alt="">
        </a>
      </div>
      <div class="navbar-inner">
        <!-- Collapse -->
        <div class="collapse navbar-collapse" id="sidenav-collapse-main">
          <!-- Nav items -->
          <ul class="navbar-nav">                     
            <li class="nav-item">
              <a class="nav-link" href="/home/all_product">
                <i class="ni ni-bullet-list-67 text-default"></i>
                <span class="nav-link-text">All Products</span>
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/home/add_product">
                <i class="ni ni-planet text-orange"></i>
                <span class="nav-link-text">Add New Products</span>
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/home/user">
                <i class="fa fa-user text-blue"></i>
                <span class="nav-link-text">Users</span>
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/home/review">
                <i class="fa fa-comment text-green"></i>
                <span class="nav-link-text">Reviews</span>
              </a>
            </li>
           
            <li class="nav-item">
              <a class="nav-link" href="/logout">
                <i class="ni ni-key-25 text-info"></i>
                <span class="nav-link-text">Logout</span>
              </a>
            </li>
            
            
          </ul>
        </div>
      </div>
    </div>
  </nav>
  <!-- Main content -->
  <div class="main-content" id="panel">
    <!-- Topnav -->
    
    <!-- Page content -->
    <div class="container-fluid mt--6">
      <div class="row">
        
        <div class="col-xl-8 order-xl-1">
          <div class="card" style="margin-top: 85px;">
            <div class="card-body">
              <form method="POST">
                <h6 class="heading-small text-muted mb-4">Product information</h6>
                <div class="pl-lg-4">
                  <div class="row">
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label" for="name">Product Name</label>
                        <input type="text" name="name" class="form-control" placeholder="Product Name" ">
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label" for="price">Price</label>
                        <input type="text" name="price" class="form-control" placeholder="Price">
                      </div>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label" for="brand">Brand Name</label>
                        <input type="text" name="brand" class="form-control" placeholder="Asus,Acer,Coorsair">
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label" for="type">Product Type</label>
                        <input type="text" name="type" class="form-control" placeholder="Monitor,Ram,Storage">
                      </div>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label" for="subtype">Sub Type</label>
                        <input type="text" name="subtype" class="form-control" placeholder="Desktop,Laptop">
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label" for="image">Image Name</label>
                        <input type="text" name="image" class="form-control" placeholder="ex.jpg">
                      </div>
                    </div>
                  </div>
                </div>

                
                <hr class="my-4" />
                <!-- Description -->
                <h6 class="heading-small text-muted mb-4">Description</h6>
                <div class="pl-lg-4">
                  <div class="form-group">
                    <label class="form-control-label">Description</label>
                    <textarea rows="4" name="description" class="form-control" placeholder="Add Description."></textarea>
                  </div>
                </div>
                <button type="submit" class="btn btn-primary my-4">
                  Submit
                </button>
              </form>
            </div>
          </div>
        </div>
      </div>
      <!-- Footer -->
      <footer class="footer pt-0">
        <div class="row align-items-center justify-content-lg-between">
          <div class="col-lg-6">
            <div class="copyright text-center  text-lg-left  text-muted">
              &copy; 2020 <a href="https://www.creative-tim.com" class="font-weight-bold ml-1" target="_blank">Creative Tim</a>
            </div>
          </div>
          <div class="col-lg-6">
            <ul class="nav nav-footer justify-content-center justify-content-lg-end">
              <li class="nav-item">
                <a href="https://www.creative-tim.com" class="nav-link" target="_blank">Creative Tim</a>
              </li>
              <li class="nav-item">
                <a href="https://www.creative-tim.com/presentation" class="nav-link" target="_blank">About Us</a>
              </li>
              <li class="nav-item">
                <a href="http://blog.creative-tim.com" class="nav-link" target="_blank">Blog</a>
              </li>
              <li class="nav-item">
                <a href="https://github.com/creativetimofficial/argon-dashboard/blob/master/LICENSE.md" class="nav-link" target="_blank">MIT License</a>
              </li>
            </ul>
          </div>
        </div>
      </footer>
    </div>
  </div>
  <!-- Argon Scripts -->
  <!-- Core -->
  <script src="/abc/vendor/jquery/dist/jquery.min.js"></script>
  <script src="/abc/vendor/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
  <script src="/abc/vendor/js-cookie/js.cookie.js"></script>
  <script src="/abc/vendor/jquery.scrollbar/jquery.scrollbar.min.js"></script>
  <script src="/abc/vendor/jquery-scroll-lock/dist/jquery-scrollLock.min.js"></script>
  <!-- Argon JS -->
  <script src="/abc/js/argon.js?v=1.2.0"></script>
</body>

</html>